<UserControl x:Class="PracticaUsuarios.UserControl2"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:PracticaUsuarios"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid Background="white">
        <StackPanel>
            <TextBlock>Nombre:</TextBlock>
            <TextBox Width="250" HorizontalAlignment="Left"></TextBox>
            <TextBlock>Telefono:</TextBlock>
            <TextBox Width="250" HorizontalAlignment="Left"></TextBox>
            <TextBlock>Cédula Profesional:</TextBlock>
            <TextBox Width="250" HorizontalAlignment="Left"></TextBox>
            <TextBlock>Carrera estudiada:</TextBlock>
            <TextBox Width="250" HorizontalAlignment="Left"></TextBox>
            <WrapPanel>
            <Button Width="75" Margin="175,10,0,0">Guardar</Button>
            </WrapPanel>
        </StackPanel>
    </Grid>
</UserControl>
